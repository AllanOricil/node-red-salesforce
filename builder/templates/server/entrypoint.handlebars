import { createNodeRedNodeMixin } from "@allanoricil/node-red-node";

{{#each nodes}}
  import {{this.name}} from "{{this.path}}";
{{/each}}

export default function (RED) { const NodeRedNodeMixin =
createNodeRedNodeMixin(RED);
{{#each nodes}}
  if({{this.name}}.credentials){ RED.nodes.registerType("{{this.id}}",
  NodeRedNodeMixin({{this.name}}), { credentials: {{this.name}}.credentials()
  }); }else{ RED.nodes.registerType("{{this.id}}", NodeRedNodeMixin({{this.name}}));
  }
{{/each}}
}